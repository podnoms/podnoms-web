{
    "index": "/index.html",
    "dataGroups": [{
        "name": "audio",
        "urls": ["https://podnomscdn.blob.core.windows.net/audio/**"],
        "cacheConfig": {
            "timeout": "10s",
            "maxSize": 0,
            "maxAge": "0u",
            "strategy": "freshness"
        }
    }],
    "assetGroups": [{
            "name": "app",
            "installMode": "prefetch",
            "resources": {
                "files": ["/favicon.ico", "/index.html", "/*.css", "/*.js"]
            }
        },
        {
            "name": "assets",
            "installMode": "lazy",
            "updateMode": "prefetch",
            "resources": {
                "files": ["/assets/**"]
            }
        }
    ]
}
